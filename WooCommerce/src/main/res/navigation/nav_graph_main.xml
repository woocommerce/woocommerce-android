            app:argType="integer" />
        <argument
            android:name="heading"
            android:defaultValue="0"
            app:argType="integer" />
        <argument
            android:name="message"
            android:defaultValue="0"
            app:argType="integer" />
        <argument
            android:name="linkTitle"
            android:defaultValue="0"
            app:argType="integer" />
        <argument
            android:name="imageResource"
            android:defaultValue="0"
